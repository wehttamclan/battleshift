<%= form_tag("/users", method: :post) do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= label_tag :password %>
  <%= password_field_tag :password%>
  <%= label_tag :password_confirmation %>
  <%= password_field_tag :password_confirmation%>
  <%= submit_tag "Submit" %>
<% end %>
